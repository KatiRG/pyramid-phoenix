<span  tal:define="oid oid|field.oid;
                   title title|field.title;
                   css_class css_class|field.widget.css_class;">
  <ul
     class="deformSet">
     ${field.start_rename()}
     <tal:loop tal:repeat="choice values">
       <tal:def tal:define="(value, title) choice">
         <li class="deformSet-item">
           <input tal:attributes="checked value == cstruct;
                                  class css_class"
                  type="radio"
                  name="${oid}"
                  value="${value}"
                  id="${oid}-${repeat.choice.index}"/>
           <label for="${oid}-${repeat.choice.index}">${title}</label>
         </li>
       </tal:def>
     </tal:loop>
     ${field.end_rename()}
  </ul>

  <script type="text/javascript">
    deform.addCallback(
      '${oid}',
      function (oid) {
        $.EsgSearch( {oid: oid } );
    });
  </script>
</span>

