<metal:block use-macro="main_template">
        
  <div metal:fill-slot="content">
    <ul class="breadcrumb">
      <li><a href="/">Home</a> <span class="divider">/</span></li>              
      <li><a href="/wizard">Wizard</a> <span class="divider">/</span></li>
      <li class="active">${title}</li>
    </ul>

    <div class="page-header">
      <h1>${title} <small>${description}</small></h1>
    </div>    

    <div class="row-fluid">
      <div class="span4" id="div-map">Map</div>
      <div class="span8">
	<form class="form-search">
	  <input type="text" class="input-medium search-query">
	  <button type="submit" class="btn">Search</button>

	  <table class="table table-bordered table-striped" id="table-csw-results">
	    <tal:rows replace="structure grid" />
	  </table>

	  <div class="pagination">
	    <ul>
	      <li><a href="#">Prev</a></li>
	      <li><a href="#">Next</a></li>
	    </ul>
	  </div>
	</form>
      </div>
    </div>

    <div class="well well-small">
      <button class="btn disabled">Previous</button>
      <button class="btn btn-primary">Next</button>
      <span/>
      <button class="btn disabled">Cancel</button>
    </div>

  </div>
   
</metal:block>
