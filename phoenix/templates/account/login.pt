<metal:block use-macro="main_template">
  <div metal:fill-slot="javascript">
    <script src="${request.static_url('phoenix:static/scripts/authomatic.js')}"></script>
    <script src="${request.static_url('phoenix:static/scripts/signin.js')}"></script>
  </div>
 
  <div metal:fill-slot="content">
    <ul class="nav nav-tabs">
      <li class="${'active' if active=='esgf' else ''}"><a href="${request.route_path('account_login', protocol='esgf')}">ESGF</a></li>
      <li class="${'active' if active=='openid' else ''}"><a href="${request.route_path('account_login', protocol='openid')}">OpenID</a></li>
    </ul>
    
     <!-- row columns -->
     <div class="row-fluid offset1">
       ${panel('headings')}
     </div>
  </div>

</metal:block>
