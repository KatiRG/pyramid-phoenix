<metal:block use-macro="main_template">

  <div metal:fill-slot="content">

    <ul class="breadcrumb">
      <li><a href="/">Home</a> <span class="divider">/</span></li>
      <li class="active">${title}</li>
    </ul>

    <div class="page-header">
      <h1>${title} <small>${description}</small></h1>
    </div>    

     <!-- row columns -->
     <div class="row">
       ${panel('headings')}
     </div>

  </div>
   
</metal:block>
