<metal:block use-macro="main_template">

  <div metal:fill-slot="content">
    
    <ul class="nav nav-tabs">
      <li class="${'active' if active=='jobs' else ''}"><a href="${request.route_url('dashboard', tab='jobs')}">Jobs</a></li>
      <li class="${'active' if active=='users' else ''}"><a href="${request.route_url('dashboard', tab='users')}">Users</a></li>
      <li class="${'active' if active=='processes' else ''}"><a href="${request.route_url('dashboard', tab='processes')}">Processes</a></li>
      <li class="${'active' if active=='downloads' else ''}"><a href="${request.route_url('dashboard', tab='downloads')}">Downloads</a></li>
    </ul>

     <!-- row columns -->
     <div class="row-fluid offset1">
       ${panel('headings')}
     </div>

  </div>
   
</metal:block>
