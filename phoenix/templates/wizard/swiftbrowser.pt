<metal:block use-macro="main_template">
        
  <div metal:fill-slot="content">

    <ul class="breadcrumb">
      <li><a href="${request.route_url('wizard_swiftbrowser')}">Containers</a></li> 

      <li tal:condition="container"><span class="divider">/</span>
        <a class="u" href="${request.route_url('wizard_swiftbrowser', _query=[('container', container)])}">${container}</a>
      </li>

      <li tal:repeat="prefix prefixes">
        <span class="divider">/</span>
        <a href="${request.route_url('wizard_swiftbrowser', _query=[('container', container), ('prefix', prefix.full_name)])}">${prefix.display_name}</a>
      </li>
    </ul> 

    <table class="table table-striped">
      <tal:rows replace="structure grid" />
    </table>
     
  </div>
   
</metal:block>
