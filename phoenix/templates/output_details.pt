<metal:block use-macro="main_template">
	
  <div metal:fill-slot="content">
    <legend>
      <h3>${title}</h3>
      <small>${form_info}</small>
    </legend>
            
    <table id="job_details_table" class="table table-condensed accordion-group" style="table-layout: fixed; word-wrap: break-word;">
      <thead>
        <tr>
          <th>Output</th>
          <th>Mime Type</th>
        </tr>
      </thead>
      <tbody>
        <div tal:repeat="output outputs">
          <tr>
            <td><a href="${output.reference}">${output.identifier}</a></td>
            <td>${output.mimeType}</td>
	  <tr>                    
        </div>
      </tbody>
    </table>
    
  </div>
   
</metal:block>

