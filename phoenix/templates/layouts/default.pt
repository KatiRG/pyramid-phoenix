<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>${layout.project_title}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles
    ======================================================================= -->

    <!-- Generated resources -->
    <tal:resources repeat="css_path css_resources | []">
      <link rel="stylesheet" media="screen" type="text/css"
            tal:attributes="href python: request.static_url(css_path)" />
    </tal:resources>

    <link rel="stylesheet" type="text/css" media="screen" charset="utf-8"
          href="${request.static_url('phoenix:static/themes/smoothness/jquery-ui.css')}" />

    <link rel="stylesheet" type="text/css" media="screen" charset="utf-8"
          href="${request.static_url('deform_bootstrap:static/deform_bootstrap.css')}" />

    <link rel="stylesheet" type="text/css" media="screen" charset="utf-8"
          href="${request.static_url('deform_bootstrap:static/bootstrap-datepicker.css')}" />

    <link rel="stylesheet" type="text/css" media="screen" charset="utf-8"
          href="${request.static_url('phoenix:static/bootstrap-tagmanager/bootstrap-tagmanager.css')}" />

    <link rel="stylesheet" type="text/css" media="screen" charset="utf-8"
          href="${request.static_url('phoenix:static/css/deform_phoenix.css')}" />

    <link rel="stylesheet" type="text/css" media="screen" charset="utf-8"
          href="${request.static_url('phoenix:static/bootglyph/css/icon.css')}" />
    
    <metal:css define-slot="css"/>

    <!-- Le javascript, which unfortunately has to be at the top for Deform to work -->
    <script src="${request.static_url('deform_bootstrap:static/jquery-1.7.1.min.js')}"></script>
    <script src="${request.static_url('deform_bootstrap:static/jquery-ui-1.8.18.custom.min.js')}"></script>
    <script src="${request.static_url('deform:static/scripts/deform.js')}"></script>

    <script src="${request.static_url('deform_bootstrap:static/jquery.maskedinput-1.3.js')}"></script>
    <script src="${request.static_url('deform_bootstrap:static/deform_bootstrap.js')}"></script>
    <script src="${request.static_url('deform_bootstrap:static/bootstrap.min.js')}"></script>
    <script src="${request.static_url('deform_bootstrap:static/bootstrap-typeahead.js')}"></script>
    <script src="${request.static_url('deform_bootstrap:static/bootstrap-datepicker.js')}"></script>

    <script src="${request.static_url('phoenix:static/bootstrap-tagmanager/bootstrap-tagmanager.js')}"></script>
    <script src="${request.static_url('deform:static/tinymce/jscripts/tiny_mce/tiny_mce.js')}" type="text/javascript"></script>
    <script src="${request.static_url('phoenix:static/js/esgsearch.js')}"></script>
    <script src="${request.static_url('phoenix:static/js/esgsearch-ui.js')}"></script>

    <!-- Generated Resources -->
    <tal:resources repeat="js_path js_resources | []">
        <script type="text/javascript"
                tal:attributes="src request.static_url(js_path)"></script>
    </tal:resources>

    <metal:js define-slot="javascript"/>   
    
  </head>

    <body>

        ${panel('navbar')}
    
        <div class="container">
    
            <div class="alert ${message_type}" tal:condition="message">
                <a class="close" data-dismiss="alert" href="#">x</a>${message}
            </div>
    
            <metal:content define-slot="content"/>
    
            ${panel('footer')}
    
        </div> <!-- /container -->

    </body>
</html>
