<metal:block use-macro="main_template">

  <div metal:fill-slot="content">

    <div class="panel panel-default">
      <div class="panel-heading">
        <h4>${name}
          <a class="btn btn-success pull-right" href="${request.route_path('supervisor_log', name=name, offset=offset)}"><i class="fa fa-lg fa-repeat"></i></a>
        </h4>
      </div>
      <div class="panel-body">
        <pre class="pre-scrollable log-output">
          ${"\n".join( ["{0:3d} {1}".format(i+1, log[i]) for i in range(len(log))] )}
        </pre>
      </div>
    </div>

  </div>
   
</metal:block>
